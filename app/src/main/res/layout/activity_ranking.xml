<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="5dp"
    android:background="@drawable/ui_frame_dark_transparent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/ranking_text_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="30dp"
        android:gravity="center"
        android:text="Ranking"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ranking_text_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="20sp"
        android:layout_margin="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/ranking_text_2"
        app:layout_constraintBottom_toTopOf="@id/ranking_text_3"
        app:layout_constraintHorizontal_weight="1"
        />

    <TextView
        android:id="@+id/ranking_text_2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="20sp"
        android:layout_margin="20dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@id/ranking_text_1"
        app:layout_constraintBottom_toTopOf="@id/ranking_text_4"
        app:layout_constraintHorizontal_weight="1"
        />

    <TextView
        android:id="@+id/ranking_text_3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="20sp"
        android:layout_margin="20dp"
        app:layout_constraintBottom_toTopOf="@id/ranking_text_5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/ranking_text_4"
        app:layout_constraintHorizontal_weight="1"
        />

    <TextView
        android:id="@+id/ranking_text_4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView333"
        android:textSize="20sp"
        android:layout_margin="20dp"
        app:layout_constraintBottom_toTopOf="@id/ranking_text_6"
        app:layout_constraintLeft_toRightOf="@id/ranking_text_3"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_weight="1"
        />

    <TextView
        android:id="@+id/ranking_text_5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView55556"
        android:textSize="20sp"
        android:layout_margin="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/ranking_text_6"
        app:layout_constraintHorizontal_weight="1"/>

    <TextView
        android:id="@+id/ranking_text_6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="20sp"
        android:layout_margin="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/ranking_text_5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_weight="1"/>


    <TextView
        android:id="@+id/ranking_text_7"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="20sp"
        android:layout_margin="20dp"
        app:layout_constraintTop_toBottomOf="@id/ranking_text_5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/ranking_text_8"
        app:layout_constraintHorizontal_weight="1"
        />

    <TextView
        android:id="@+id/ranking_text_8"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="20sp"
        android:layout_margin="20dp"
        app:layout_constraintTop_toBottomOf="@id/ranking_text_6"
        app:layout_constraintLeft_toRightOf="@id/ranking_text_7"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_weight="1"
        />

    <TextView
        android:id="@+id/ranking_text_9"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="20sp"
        android:layout_margin="20dp"
        app:layout_constraintTop_toBottomOf="@id/ranking_text_7"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/ranking_text_10"
        app:layout_constraintHorizontal_weight="1"
        />

    <TextView
        android:id="@+id/ranking_text_10"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView22222"
        android:textSize="20sp"
        android:layout_margin="20dp"
        app:layout_constraintTop_toBottomOf="@id/ranking_text_8"
        app:layout_constraintLeft_toRightOf="@id/ranking_text_9"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_weight="1"
        />

    <Button
        android:id="@+id/ranking_button_reset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reset"
        android:gravity="center"
        android:background="@drawable/button_green"
        android:layout_margin="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/ranking_button_back"
        />

    <Button
        android:id="@+id/ranking_button_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back"
        android:gravity="center"
        android:background="@drawable/button_green"
        android:layout_margin="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/ranking_button_reset"
        app:layout_constraintRight_toRightOf="parent"/>

</android.support.constraint.ConstraintLayout>